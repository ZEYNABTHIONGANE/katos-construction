{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": false,
    "lib": [
      "es2017",
      "dom"
    ],
    "target": "es2017",
    "jsx": "react-native",
    "module": "esnext",
    "moduleResolution": "bundler",
    "allowImportingTsExtensions": true,
    "resolveJsonModule": true,
    "noEmit": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "*": [
        "src/*"
      ]
    }
  }
}